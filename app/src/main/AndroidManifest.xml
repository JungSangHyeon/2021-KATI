<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.plim.kati_app">

    <uses-permission android:name="android.permission.WAKE_LOCK" />


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KATI_app"
        android:usesCleartextTraffic="true">


        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->


        <activity
            android:name=".domain.MainActivity"
            android:label="mainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".domain.food.search.FoodSearchActivity" />
        <activity android:name=".domain.user.signOut.WithdrawalActivity" />
        <activity android:name=".domain.user.signOut.TempActivity" />
        <activity android:name=".domain.user.login.LoginActivity" />
        <activity android:name=".domain.user.register.RegisterActivity" />
        <activity android:name=".domain.user.logOut.LogOutActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>